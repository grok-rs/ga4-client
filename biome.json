{
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
  "organizeImports": { "enabled": true },
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true,
      "correctness": { "noUnusedVariables": "error", "noUnusedImports": "error" }
    }
  },
  "formatter": { "lineWidth": 120 },
  "javascript": { "formatter": { "quoteStyle": "single" } }
}
