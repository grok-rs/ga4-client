{
  "name": "ga4-client",
  "version": "1.0.0",
  "description": "Type-safe GA4 Measurement Protocol client for Node.js",
  "type": "module",
  "exports": {
    ".": {
      "import": { "types": "./dist/index.d.ts", "default": "./dist/index.js" },
      "require": { "types": "./dist/index.d.cts", "default": "./dist/index.cjs" }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "node --import tsx --test src/**/*.test.ts",
    "lint": "biome check src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["google-analytics", "ga4", "measurement-protocol", "analytics", "typescript"],
  "author": "grok-rs",
  "license": "MIT",
  "repository": { "type": "git", "url": "https://github.com/grok-rs/ga4-client.git" },
  "bugs": { "url": "https://github.com/grok-rs/ga4-client/issues" },
  "homepage": "https://github.com/grok-rs/ga4-client#readme",
  "engines": { "node": ">=22" },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
